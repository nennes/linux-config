[global_config]
[keybindings]
  copy = <Primary>c
  paste = <Primary>v
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    copy_on_selection = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
